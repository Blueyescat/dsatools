{
  "env": {
    "es2022": true,
    "browser": true,
    "worker": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "indent": [ "error", "tab", { "SwitchCase": 1 } ],
    "quotes": [ "error", "double" ],
    "semi": [ "error", "never" ],
    "prefer-const": "warn"
  }
}
